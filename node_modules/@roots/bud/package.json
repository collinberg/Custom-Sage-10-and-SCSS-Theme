{
  "name": "@roots/bud",
  "version": "6.8.0",
  "description": "Frontend build tools combining the best parts of Symfony Encore and Laravel Mix",
  "homepage": "https://roots.io/bud",
  "contributors": [
    {
      "name": "Kelly Mears",
      "email": "developers@tinypixel.dev",
      "url": "https://github.com/kellymears"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "url": "https://github.com/retlehs"
    },
    {
      "name": "QWp6t",
      "url": "https://github.com/QWp6t"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "sources/@roots/bud"
  },
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "keywords": [
    "bud",
    "roots",
    "webpack"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "bin/",
    "docs/",
    "lib/",
    "config/",
    "scripts/",
    "vendor/"
  ],
  "bin": {
    "bud": "./bin/bud.mjs",
    "bund": "./bin/bund.mjs",
    "ts-bud": "./bin/ts-bud.mjs"
  },
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./bud": {
      "types": "./lib/bud/bud.d.ts",
      "default": "./lib/bud/bud.js"
    },
    "./instances": {
      "types": "./lib/instances.d.ts",
      "default": "./lib/instances.js"
    },
    "./cli/*": {
      "types": "./lib/cli/*.d.ts",
      "default": "./lib/cli/*.js"
    },
    "./cli/commands/*": {
      "types": "./lib/cli/commands/*.d.ts",
      "default": "./lib/cli/commands/*.js"
    },
    "./cli/decorators": {
      "types": "./lib/cli/decorators/index.d.ts",
      "default": "./lib/cli/decorators/index.js"
    },
    "./cli/decorators/*": {
      "types": "./lib/cli/decorators/*.d.ts",
      "default": "./lib/cli/decorators/*.js"
    },
    "./cli/helpers/*": {
      "types": "./lib/cli/helpers/*.d.ts",
      "default": "./lib/cli/helpers/*.js"
    },
    "./config/jsconfig.json": {
      "default": "./config/jsconfig.json"
    },
    "./config/tsconfig.json": {
      "default": "./config/tsconfig.json"
    },
    "./context": {
      "types": "./lib/context/index.d.ts",
      "default": "./lib/context/index.js"
    },
    "./context/*": {
      "types": "./lib/context/*.d.ts",
      "default": "./lib/context/*.js"
    },
    "./factory": {
      "types": "./lib/factory/index.d.ts",
      "default": "./lib/factory/index.js"
    },
    "./factory/*": {
      "types": "./lib/factory/*.d.ts",
      "default": "./lib/factory/*.js"
    },
    "./logger": {
      "types": "./lib/logger/index.d.ts",
      "default": "./lib/logger/index.js"
    },
    "./notifier/*": {
      "types": "./lib/notifier/*.d.ts",
      "default": "./lib/notifier/*.js"
    },
    "./services/*": {
      "types": "./lib/services/*/index.d.ts",
      "default": "./lib/services/*/index.js"
    },
    "./services/*/*": {
      "types": "./lib/services/*/*.d.ts",
      "default": "./lib/services/*/*.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "lib/index.d.ts"
      ],
      "cli": [
        "lib/cli/index.d.ts"
      ],
      "cli/commands": [
        "lib/cli/commands/index.d.ts"
      ],
      "cli/decorators": [
        "lib/cli/decorators/index.d.ts"
      ],
      "context": [
        "lib/context/index.d.ts"
      ],
      "factory": [
        "lib/factory/index.d.ts"
      ],
      "factory/*": [
        "lib/factory/*.d.ts"
      ],
      "logger": [
        "lib/logger/index.d.ts"
      ],
      "notifier": [
        "lib/notifier/index.d.ts"
      ],
      "services": [
        "lib/services/index.d.ts"
      ],
      "services/*": [
        "lib/services/*/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@skypack/package-check": "0.2.2",
    "@types/node": "18.11.18"
  },
  "dependencies": {
    "@roots/browserslist-config": "6.8.0",
    "@roots/bud-api": "6.8.0",
    "@roots/bud-build": "6.8.0",
    "@roots/bud-cache": "6.8.0",
    "@roots/bud-client": "6.8.0",
    "@roots/bud-compiler": "6.8.0",
    "@roots/bud-dashboard": "6.8.0",
    "@roots/bud-entrypoints": "6.8.0",
    "@roots/bud-extensions": "6.8.0",
    "@roots/bud-framework": "6.8.0",
    "@roots/bud-hooks": "6.8.0",
    "@roots/bud-server": "6.8.0",
    "@roots/bud-support": "6.8.0",
    "@roots/bud-terser": "6.8.0",
    "browserslist": "4.21.4",
    "execa": "6.1.0",
    "tslib": "2.4.1",
    "update-browserslist-db": "1.0.10"
  },
  "peerDependencies": {
    "@roots/browserslist-config": "*",
    "@roots/bud-api": "*",
    "@roots/bud-build": "*",
    "@roots/bud-cache": "*",
    "@roots/bud-client": "*",
    "@roots/bud-compiler": "*",
    "@roots/bud-dashboard": "*",
    "@roots/bud-entrypoints": "*",
    "@roots/bud-extensions": "*",
    "@roots/bud-framework": "*",
    "@roots/bud-hooks": "*",
    "@roots/bud-server": "*",
    "@roots/bud-support": "*",
    "@roots/bud-terser": "*",
    "browserslist": ">=4.21.4",
    "execa": ">=6.1.0",
    "tslib": ">=2.4.0",
    "update-browserslist-db": ">=1.0.10"
  },
  "peerDependenciesMeta": {
    "@roots/browserslist-config": {
      "optional": true
    },
    "@roots/bud-api": {
      "optional": true
    },
    "@roots/bud-build": {
      "optional": true
    },
    "@roots/bud-cache": {
      "optional": true
    },
    "@roots/bud-client": {
      "optional": true
    },
    "@roots/bud-compiler": {
      "optional": true
    },
    "@roots/bud-dashboard": {
      "optional": true
    },
    "@roots/bud-entrypoints": {
      "optional": true
    },
    "@roots/bud-extensions": {
      "optional": true
    },
    "@roots/bud-framework": {
      "optional": true
    },
    "@roots/bud-hooks": {
      "optional": true
    },
    "@roots/bud-server": {
      "optional": true
    },
    "@roots/bud-support": {
      "optional": true
    },
    "@roots/bud-terser": {
      "optional": true
    },
    "browserslist": {
      "optional": true
    },
    "execa": {
      "optional": true
    },
    "tslib": {
      "optional": true
    },
    "update-browserslist-db": {
      "optional": true
    }
  },
  "scripts": {
    "postinstall": "node scripts/postinstall.mjs"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}