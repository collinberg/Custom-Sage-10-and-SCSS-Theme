  "license": "MIT",
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "bud",
    "bud-framework"
  ],
  "files": [
    "lib/",
    "vendor/"
  ],
  "type": "module",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./api": {
      "types": "./lib/api/index.d.ts",
      "default": "./lib/api/index.js"
    },
    "./methods": {
      "types": "./lib/methods/index.d.ts",
      "default": "./lib/methods/index.js"
    },
    "./methods/*": {
      "types": "./lib/methods/*.d.ts",
      "default": "./lib/methods/*.js"
    }
  },
  "devDependencies": {
    "@roots/bud-extensions": "6.8.0",
    "@roots/bud-terser": "6.8.0",
    "@skypack/package-check": "0.2.2",
    "@types/node": "18.11.18"
  },
  "dependencies": {
    "@roots/bud-framework": "6.8.0",
    "@roots/bud-support": "6.8.0",
    "@roots/container": "6.8.0"
  },
  "volta": {
    "extends": "../../../package.json"
  }
}